{
  "name": "Zentaor",
  "description": "The best Zentao plugin.",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "declarativeContent"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "icons": {
    "16": "images/new/zentao16x16.png",
    "32": "images/new/zentao32x32.png",
    "48": "images/new/zentao48x48.png",
    "128": "images/new/zentao128x128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "The best Zentao plugin",
    "default_icon": {
      "16": "images/new/zentao16x16.png",
      "32": "images/new/zentao32x32.png",
      "48": "images/new/zentao48x48.png",
      "128": "images/new/zentao128x128.png"
    }
  },

  "content_scripts": [
    {
      "js": [
        "js/dayjs.min.js",
        "js/axios.min.js",
        "js/index.js"
      ],
      "matches": [
        "http://zentao.xzxyun.com/zentao/*"
      ],
      "css": [
        "css/item.css"
      ]
    }
  ]
}